{
    "name": "@gigya-ts/gigya",
    "version": "0.1.19",
    "description": "Type-safe Gigya client for the browser and Node.js",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "!**/*.tsbuildinfo",
        "README.md"
    ],
    "scripts": {
        "build": "tsc",
        "lint": "eslint \"**/*.ts*\"",
        "test": "node --env-file .env --import tsx --test --test-reporter spec ./src/**/*.test.ts"
    },
    "dependencies": {
        "@gigya-ts/rest-api": "workspace:*"
    },
    "devDependencies": {
        "eslint-config-custom": "workspace:*",
        "tsconfig": "workspace:*",
        "tsx": "4.19.2"
    }
}
